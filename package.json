{
  "name": "SSW.Rules",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build:dir": "mkdirp ./public/icons/",
    "build:favicons": "node scripts/favicons",
    "build": "yarn run build:dir && yarn run build:favicons && gatsby build --prefix-paths --log-pages",
    "build:incremental": "gatsby build --prefix-paths --log-pages",
    "devbuild": "gatsby clean && yarn run build:dir && yarn run build:favicons && gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "yarn run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "lint": "eslint --max-warnings=0 . --ext ts --ext tsx --ext js --ext jsx"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.4.0",
    "@microsoft/applicationinsights-web": "^2.6.1",
    "@raae/gatsby-remark-oembed": "^0.1.1",
    "applicationinsights": "^1.8.10",
    "autoprefixer": "^9.7.6",
    "classnames": "^2.3.1",
    "core-js": "^3.10.2",
    "date-fns": "^2.21.1",
    "directory-named-webpack-plugin": "^4.0.1",
    "favicons": "^6.2.1",
    "gatsby": "2.28",
    "gatsby-custom-md": "^1.3.0",
    "gatsby-plugin-breadcrumb": "^12.1.1",
    "gatsby-plugin-google-analytics": "^3.4.0",
    "gatsby-plugin-netlify-cms": "4.10.1",
    "gatsby-plugin-postcss": "3.x.x",
    "gatsby-plugin-react-helmet": "^4.4.0",
    "gatsby-plugin-sharp": "^3.4.0",
    "gatsby-plugin-sitemap": "^4.0.0",
    "gatsby-remark-copy-linked-files": "^4.1.0",
    "gatsby-remark-custom-blocks": "^3.4.0",
    "gatsby-remark-embed-video": "^3.1.1",
    "gatsby-remark-fenced-divs": "^1.0.3",
    "gatsby-remark-figure-caption": "^2.0.0",
    "gatsby-remark-images": "^5.1.0",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-remark-responsive-iframe": "^4.1.0",
    "gatsby-remark-vscode": "^3.2.1",
    "gatsby-source-filesystem": "^3.4.0",
    "gatsby-source-git": "^1.0.2",
    "gatsby-source-local-git": "^1.2.2",
    "gatsby-transformer-json": "^3.4.0",
    "gatsby-transformer-remark": "3.2.0",
    "markdown-it": "^12.0.4",
    "markdown-it-container": "^3.0.0",
    "netlify-cms-app": "^2.14.46",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1",
    "preval.macro": "^5.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-recaptcha": "^2.1.0",
    "react-helmet": "^6.0.0",
    "react-modal": "^3.13.1",
    "react-number-format": "^4.5.5",
    "react-pose": "^4.0.10",
    "rehype-react": "^6.2.0",
    "sharp": "^0.28.1",
    "svg-react-loader": "^0.4.6",
    "webpack-assets-manifest": "4.0.6"
  },
