{
  "name": "SSW.Rules",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build:dir": "mkdirp ./public/icons/",
    "build:favicons": "node scripts/favicons",
    "build": "yarn run build:dir && yarn run build:favicons && gatsby build --prefix-paths --log-pages",
    "build:incremental": "gatsby build --prefix-paths --log-pages",
    "devbuild": "gatsby clean && yarn run build:dir && yarn run build:favicons && gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "yarn run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "lint": "eslint --max-warnings=0 . --ext ts --ext tsx --ext js --ext jsx"
  },
  "dependencies": {
    "@microsoft/applicationinsights-web": "^2.5.9",
    "applicationinsights": "^1.8.8",
    "autoprefixer": "^9.7.6",
    "classnames": "^2.2.6",
    "favicons": "^6.0.0",
    "gatsby": "^2.21.0",
    "gatsby-custom-md": "^1.3.0",
    "gatsby-plugin-breadcrumb": "^10.0.0",
    "gatsby-plugin-postcss": "^2.3.0",
    "gatsby-plugin-react-helmet": "^3.3.0",
    "gatsby-plugin-sharp": "^2.6.14",
    "gatsby-plugin-sitemap": "^2.4.0",
    "gatsby-remark-copy-linked-files": "^2.3.10",
    "gatsby-remark-custom-blocks": "^2.3.9",
    "gatsby-remark-embed-video": "^3.0.10",
    "gatsby-remark-fenced-divs": "^1.0.3",
    "gatsby-remark-figure-caption": "^2.0.0",
    "gatsby-remark-images": "^3.3.13",
    "gatsby-remark-prismjs": "^3.5.9",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-remark-responsive-iframe": "^2.4.10",
    "gatsby-source-filesystem": "^2.3.8",
    "gatsby-source-git": "^1.0.2",
    "gatsby-source-local-git": "^1.0.0",
    "gatsby-transformer-json": "^2.4.0",
    "gatsby-transformer-remark": "^2.8.13",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1",
    "preval.macro": "^5.0.0",
    "prismjs": "^1.20.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-ga": "^3.1.2",
    "react-google-recaptcha": "^2.1.0",
    "react-helmet": "^6.0.0",
    "react-modal": "^3.11.2",
    "react-number-format": "^4.4.1",
    "react-pose": "^4.0.10",
    "rehype-react": "^6.1.0",
    "svg-react-loader": "^0.4.6",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.31",
    "@fortawesome/free-brands-svg-icons": "^5.15.0",
    "@fortawesome/free-solid-svg-icons": "^5.15.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "tailwindcss": "^1.4.0"
  }
}
